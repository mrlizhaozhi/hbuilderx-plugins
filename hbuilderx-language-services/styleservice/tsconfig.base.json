{
	"compilerOptions": {
		"target": "es2019",
		"lib": [
			"es2019"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"composite": true,
		"declaration": true,
		"strict": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules"
	]
}